pipeline {

    agent any
    tools {
        maven 'apache-maven-3.6.3' 
    }
    stages {
        stage('Compile stage') {
            steps {
                bat "mvn compile verify sonar:sonar" 
        }
    }

         stage('testing stage') {
             steps {
                bat "mvn test verify sonar:sonar"
        }
    }
    stage('artifactory') {
    steps{
    rtUpload (
    serverId: 'art-1',
    spec: '''{
          "files": [
            {
              "pattern": "*.war",
              "target": "kapil/"
            }
         ]
    }'''
    
    )
    }
    }

          stage('deployment stage') {
              steps {
                bat "mvn tomcat7:run"
        }
    }
    }
    }
